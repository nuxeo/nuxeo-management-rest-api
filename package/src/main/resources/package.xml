<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-management-rest-api" version="@VERSION@">
<title>Nuxeo Management REST API</title>
<description>
  <p>The Management REST API adds a set of REST endpoints allowing to manage the Nuxeo Platform.</p>
</description>
<home-page>https://doc.nuxeo.com/nxdoc/nuxeo-csv</home-page>
<vendor>Nuxeo</vendor>
<installer restart="true" />
<uninstaller restart="true" />
<nuxeo-validation>nuxeo_certified</nuxeo-validation>
<production-state>production_ready</production-state>
<supported>true</supported>
<platforms>
  <platform>server-@DISTRIB_VERSION@</platform>
  <platform>server-@DISTRIB_VERSION@-*</platform>
</platforms>
<license>LGPL</license>
<license-url>http://www.gnu.org/licenses/lgpl.html</license-url>
<visibility>PUBLIC</visibility>
</package>
